{% extends 'base.html.twig' %}


{% block title %}
    Application
{% endblock %}

{% block main %}
    <h5 class="modal-title text-monospace custom-m-b">{{ 'label.delete_list'|trans({'%name%': deletedList.name}) }}</h5>
    <div class="form-group">
        {{ form_start(form, { method: 'post', action: url('list_delete', {'id': deletedList.id}) }) }}
        {{ form_widget(form) }}
        <input type="submit" value="{{ 'action.delete'|trans }}" class="btn btn-danger" />
        {{ form_end(form) }}
    </div>
{% endblock %}